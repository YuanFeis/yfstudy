        <mxCell id="n56QRDZfdTH0vBFQAGBd-8" value="综合交换机技术" style="ellipse;whiteSpace=wrap;html=1;aspect=fixed;rounded=0;sketch=0;strokeColor=#457B9D;fillColor=#A8DADC;fontColor=#1D3557;" vertex="1" parent="1">
